#include <iostream>
using namespace std;
int odd_one_out(int arr[], int size)
{
    int result = 0;
    for(int i=0; i<size; i++)
    {
    	  result = result^arr[i];
    }
    return result;
}
int main() {
	
	int arr[] = {2,3,4,4,1,4,3,1,4,2};
    int num = odd_one_out(arr,10);
    cout<<num;
	return 0;
}
